[ghq]
    root = ~/dev/src/

[user]
    name = makicamel
    email = unright@gmail.com

[github]
    user = makicamel

[pull]
    rebase = true

[push]
    default = current

[branch]
    autosetuprebase = always

[color]
    ui = true

[alias]
    co = checkout
    st = status
    r = rebase
    rc = rebase --continue
    ri = rebase --interactive develop
    graph = log --pretty=format:'%Cgreen[%cd] %Cblue%h %Cred<%cn> %Creset%s' --date=short  --decorate --graph --branches --tags
    today = log --oneline --since='12 hour ago'